<div class="row">
  <div class="span6 offset3">
    <h1>Welcome to Webinar Executive Coaching</h1>
    <%= image_tag("proto3_3_1.png") %>
    <h2>Stay in touch.</h2>
  
      <%= form_for @visitor do |f| %>
        <%= render 'shared/error_messages', object: f.object %>
        <%= f.label :email %>
      <%= f.text_field :email %>
     
        <br/>
         <%= f.submit "Sign up for the newsletter", class: "btn btn-large btn-primary" %>
  
      <% end %>
    </div>
  </div>

